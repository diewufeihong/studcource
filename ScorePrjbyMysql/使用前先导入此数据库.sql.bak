);


CREATE TABLE T_SCORESYSTEMTEACHERS(
       ACCOUNT VARCHAR(25) PRIMARY KEY,
       PASSWORD VARCHAR(25) NOT NULL,
       NAME     VARCHAR(30) NOT NULL,
       AGE      VARCHAR(10) NOT NULL,
       TITLE      VARCHAR(10) NOT NULL
);

CREATE TABLE T_SCORESYSTEMADMINS(
       ACCOUNT VARCHAR(25) PRIMARY KEY,
       PASSWORD VARCHAR(25) NOT NULL,
       NAME     VARCHAR(30) NOT NULL,
       PERMISSIONS      VARCHAR(10) NOT NULL
);

CREATE TABLE T_SCORESYSTEMCOURSES(
       COUNO VARCHAR(20) PRIMARY KEY,
       COUNAME VARCHAR(30) NOT NULL,
       TEACHERNO VARCHAR(25) NOT NULL
);

CREATE TABLE T_SCORESYSTEMSCORES(
       STUNO VARCHAR(20) ,
       COUNO VARCHAR(20) ,
       SCORE float ,
       CHANGEABLE VARCHAR(5)
);

Alter table T_SCORESYSTEMSCORES
Add
